pipeline {
    agent any
    stages {
        stage('Get') {
            steps {
               sh "mvn dependency:get -DrepoUrl=http://localhost:8081/repository/maven-snapshots/ -Dartifact=org.springframework.samples:sprint-petclinic:2.3.0.BUILD-20201102.131440-1 -Ddest=component.jar"
            }
        }
    }
}
